<!doctype html>
<html>
<head>
  <title><%= page.title %> &#8212; Skills</title>

  <% if(typeof assets != "undefined" && assets.css) { %>
    <% for(var filename in assets.css) { %>
      <%- css(assets.css[filename]) %>
    <% } %>
  <% } %>

    <!--[if lte IE 8]>
      <script src="https://ajax.cdnjs.com/ajax/libs/json2/20110223/json2.js"></script>
      <script src="https://html5shiv.googlecode.com/svn/trunk/html5.js"></script>  
    <![endif]-->

  <%- js('jquery-1.8.2.min.js') %>
  <%- js('base.js') %>
</head>
<body>
  <%- body %>
  <div id="Bar">
    <div id="Nav">
      <ul>
        <li><a <%- page.title == 'index' ? 'class="active"' : '' %>href="index">Home</a></li>
        <li><a <%- page.title == 'music' ? 'class="active"' : '' %>href="Music">Music</a></li>
        <li><a <%- page.title == 'web design' ? 'class="active"' : '' %>href="WebDesign">Web Design</a></li>
        <li><a <%- page.title == 'animation' ? 'class="active"' : '' %>href="Animation">Animation</a></li>
        <li><a <%- page.title == 'resume' ? 'class="active"' : '' %>href="Resume">Resume<br /></li>
      </ul>
    </div>
    <div id="logo"><a href="index"><img src="<%- img('Logo2.png') %>"/></a></div>
  </div>
<div id="static">
  Copyright 2013 Gamersoul Software. All rights reserved.
  Website by <a href="mailto:soulesjms@gmail.com">Mark Soules</a>
</div>
  
  <% if(process.env.NODE_ENV == "production") { %>
    <%- js('clientManifest.js') %>
<% } %>

  <% if(typeof assets != "undefined"  && assets.js) { %>
    <% for(var i=0, l=assets.js.length; i<l; i++) { %>
      <%- js(assets.js[i]) %>
    <% } %>
  <% } %>
  
</body>
</html>

